	//Write a java program to find max element in a given array 
	int maxElement(int[] a)
	{
		
		//logic
		return 0;
		
		
	}
	
	//Write a java program to find a length of given name
	int findLength (String name)
	{
		//logic
		return name.length();
		
	}
	
	//write a java program 
	int sumoFTwoNumbers(int a,int b)
	{
		return a+b;
		
	}
	
	String nameConcate(String fname, String lname)
	{
		return fname+lname;
		
	}
	
	boolean eligibleVote(int age)
	{
		return age>=18;
		
	}
	
	void check(int age)
	{
		if(age>=18)
		{
			System.out.println("Eligible");
		}
		else
		{
			System.out.println("Not eligible");
		}
	}
	
	int[] arrayReverse(int [] arr)
	{
		return arr;
		//logic
		
		
	}
	
	 //write a java method to convert name into uppercase charcters
	 
	 String convertUppercase(String name)
	 {
		return name.toUpperCase();
		 
	 }
	 
	 boolean strsuffix(String name)
	 {
		 return name.endsWith(name);
	 }
	 
	
