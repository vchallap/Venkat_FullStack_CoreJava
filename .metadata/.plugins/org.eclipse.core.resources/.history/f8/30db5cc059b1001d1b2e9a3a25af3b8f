package in.coreJava.Methods;

public class Person1 {

	int personId;
	String personName;
	int personAge;

	public static void main(String[] args) {

		Person1 p1 = new Person1();
		Person1 p2 = new Person1();
		
		m1(p1,p2);

	}

	static void m1(Person1 p1, Person1 p2)

	{
		Person1 m2 = m2();
		
	}
	
	static Person1 m2()
	{
		
		Person1 p3=new Person1();
		return p3;
		
		
		
	}

	
	p1.personId = 101;
	p1.personName = "Raju";
	p1.personAge = 30;
	
	p2.personId = 102;
	p2.personName = "Rani";
	p2.personAge = 22;
	
}
