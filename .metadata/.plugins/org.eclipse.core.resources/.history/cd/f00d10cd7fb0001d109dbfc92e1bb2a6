package in.coreJava.Methods;

public class Driver7 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Driver7 d7 = new Driver7();

		DoublePersons personData = d7.getPersonData(101);
		System.out.println(personData.personName+" "+personData.personAge);
		

	}

	DoublePersons getPersonData(int personId) {
		DoublePersons dp1 = new DoublePersons();
		dp1.personId = 101;
		dp1.personName = "Raju";
		dp1.personAge = 30;
		DoublePersons dp2 = new DoublePersons();
		dp2.personId = 102;
		dp2.personName = "Rani";
		dp2.personAge = 32;

		if (personId == 101) {
			return dp1;
		} else {
			return dp2;
		}

	}

}
